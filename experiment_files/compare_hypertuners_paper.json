        },
        {
            "name": "pso_mean",
            "search_method": "pso",
            "search_method_hyperparameters": [
                {
                    "name": "popsize",
                    "value": 20
                },
                {
                    "name": "maxiter",
                    "value": 50
                },
                {
                    "name": "c1",
                    "value": 1.0
                },
                {
                    "name": "c2",
