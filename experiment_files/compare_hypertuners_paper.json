                {
                    "name": "popsize",
                    "value": 20
                },
                {
                    "name": "maxiter",
                    "value": 50
                },
                {
                    "name": "method",
                    "value": "disruptive_uniform"
                },
                {
                    "name": "mutation_chance",
                    "value": 20
                }
            ],
            "display_name": "Genetic Algorithm mean",
            "autotuner": "KernelTuner",
            "color_parent": "genetic_algorithm_tuned"
        },
        {
            "name": "pso_tuned",
