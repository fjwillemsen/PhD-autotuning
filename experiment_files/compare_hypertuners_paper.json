                {
                    "name": "popsize",
                    "value": 20
                },
                {
                    "name": "maxiter",
                    "value": 150
                },
                {
                    "name": "method",
                    "value": "single_point"
                },
                {
                    "name": "mutation_chance",
                    "value": 5
                }
            ],
            "display_name": "Genetic Algorithm optimal",
            "autotuner": "KernelTuner"
        },
        {
            "name": "genetic_algorithm_mean",
